/* components/searchNative/searchNative.wxss */
.search_nav {
  width: 100%;
  height: 90rpx;
  background: var(--themeColor);
  display: flex;
  justify-content: center;
  align-items: center;
}

.search_nav .content {
  margin: 20rpx 10rpx;
  width: 100%;
  height: 28px;
  line-height: 28px;
  background: #fff;
  border-radius: 8rpx;
  text-align: center;
  font-size: 14px;
}